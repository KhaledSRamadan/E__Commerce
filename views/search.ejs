<%- include('./_header.ejs') %>

<div class="col-md-8">
 
    <div class="panel panel-default">
 
        <div class="panel-heading">
            <h3>Search</h3>
        </div>
        <div class="panel-body">
            <form class="form-search" method="POST" action="/search">
                
                <div class="input-group">
                    <span class="input-group-addon" id="basic-addon1"><span class="glyphicon glyphicon-search"
                            aria-hidden="true"></span></span>
                    <input type="text" name="SearchInput" class="form-control" placeholder="Search item" aria-describedby="basic-addon1">
                </div>
                
 
                <input type="submit" class="btn btn-default navbar-btn" value="Search" />
 
            </form>
        </div>
    </div>
</div>
 


